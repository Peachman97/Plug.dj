        "rule":{

			"#chat .message:hover .timestamp":{
				"color":"transparent",
				"pointer-events":"none"
			},

			"#chat .text a:visited":{
				"color":"#009cdd"
			},

			"#chat .message:hover, #chat .emote:hover, #chat .update:hover":{
				"text-decoration":"none",
				"-webkit-transition":"25ms linear 5ms",
				"-moz-transition":"25ms linear 5ms",
				"-o-transition":"25ms linear 5ms",
				"transition":"25ms linear 5ms",
				"border-width": "1px 3px",
				"border-style": "solid",
				"border-color": "#00FFFF",
				"padding": "5px 2px 5px 25px"
			},

			"#chat .emote, #chat .mention, #chat .message, #chat .moderation, #chat .skip, #chat .system, #chat .update, #chat .welcome":{
				"border-radius":"20px;",
				"background-color":"#0E0000;",
				"margin-top":"2px;",
				"-webkit-animation":"fadein 0.3s",
				"-moz-animation":"fadein 0.3s",
				"-ms-animation":"fadein 0.3s",
				"-o-animation":"fadein 0.3s",
				"animation":"fadein 0.3s"
			},

			"#chat .emote:nth-child(2n+1), #chat .mention:nth-child(2n+1), #chat .message:nth-child(2n+1), #chat .moderation:nth-child(2n+1), #chat .skip:nth-child(2n+1), #chat .update:nth-child(2n+1), #chat .welcome:nth-child(2n+1)":{
				"border-radius":"20px;",
				"background-color":"#111;",
				"-webkit-animation":"fadein 0.3s",
				"-moz-animation":"fadein 0.3s",
				"-ms-animation":"fadein 0.3s",
				"-o-animation":"fadein 0.3s",
				"animation":"fadein 0.3s"
			},

			"#chat-messages":{
			   "border":"none;"
			},

			"#chat .delete-button":{
				"height": "16px",
				"width": "16px",
				"right": "12px",
				"top": "6px",
				"background": "url('http://i.imgur.com/r1Bbcv7.png') no-repeat scroll right center transparent"
			},

			"#chat .delete-button span":{
				"color": "transparent",
				"padding":"0px",
				"display":"none"
			},

			"#chat .mention.is-staff":{
			    "background-color":"rgba(99, 56, 121, 0.38) !important;",
			    "border-width": "1px 3px",
				"border-style": "solid",
				"border-color": "#AC76FF"
			},

			"#chat .welcome":{
			    "border-width": "1px 3px",
				"border-style": "solid",
				"border-color": "#AC76FF"
			},

"#chat .from .from": { "color": "#00cc00" 
			},


			"#chat .mention.is-you":{
				"background-color":"rgba(245, 255, 0, 0.1) !important;"
			},

		   "@keyframes fadein":{
			   "from {opacity":"0;}",
			   "to {opacity":"1;}"
			},

			"@-moz-keyframes fadein":{
			   "from {opacity":"0;}",
			   "to {opacity":"1;}"
			},

			"@-webkit-keyframes fadein":{
			   "from {opacity":"0;}",
			   "to {opacity":"1;}"
			},

